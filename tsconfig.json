{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/AboutSection.jsx", "src/app/apropos/components/Apropos.jsx", "src/app/components/Footer.jsx", "src/app/components/CaseStudySection.jsx", "src/app/contact/components/Contact-page.jsx", "src/app/components/ServicesSection.jsx", "src/app/components/NewsletterSection.jsx", "src/app/components/Header.jsx", "src/app/portfolio/components/Portfolio-page.jsx", "src/app/services/components/Servicespage.jsx", "src/app/services/components/Servicespage.jsx"],
  "exclude": ["node_modules"]
}
